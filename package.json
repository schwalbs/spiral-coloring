{
  "name": "sprial-coloring",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "colors:dharma": "npm run colors -- dharma",
    "colors:pro-chem": "npm run colors -- pro-chem",
    "colors": "node ./scripts/build-colors.js",
    "build:clean": "rm -rf dist/",
    "build:pwa-assets": "pwa-assets-generator",
    "prebuild": "npm run build:clean",
    "build": "tsc -b && vite build",
    "predeploy": "concurrently --names pwa-assets,build,colors -c blue,green,magenta \"npm run build:pwa-assets\" \"npm run build\" \"npm run colors\"",
    "deploy": "gh-pages -d dist",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "canvas": "^3.1.0",
    "color-sorter": "^7.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "node-html-parser": "^7.0.1",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6",
    "vite-plugin-pwa": "^1.0.0"
  }
}
